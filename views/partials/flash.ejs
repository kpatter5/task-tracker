<div class="row mt-1 mb-4 justify-content-center">
  <div class="col-6">
    <% if (success && success.length) { %>
    <div class="alert alert-success" role="alert">
      <%= success %>
    </div>

    <% } %>

    <% if (error && error.length) { %>
    <div class="alert alert-danger" role="alert">
      <%= error %>
    </div>
    <% } %>
  </div>
</div>